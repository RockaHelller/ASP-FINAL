@model SliderDetailVM
@{
    ViewData["Title"] = "Detail";
}


<div class="row">
    <div class="col-2"></div>
    <div class="col-10">
        <div class="container my-3">
            <h1 class="text-center">Slider detail</h1>
            <div class="slider-detail">
                <h3>Image</h3>
                <img src="~/images/home/index/@Model.Image" alt="image" style="width:500px;height:300px;" />
                <h3>Status : @Model.Status </h3>
                <h3>Created Date - @Model.CreateDate</h3>
            </div>
            <a asp-action="Index" class="btn btn-primary btn-rounded btn-fw">Back</a>

        </div>
    </div>
</div>
